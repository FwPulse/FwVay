  local Circle = Drawing.new("Circle")
  Circle.Transparency = Skid.FOV.Transparency
  Circle.Radius = Skid.FOV.Radius * 3
  Circle.Visible = Skid.FOV.Visible
  Circle.Color = Skid.FOV.Color
